#ifndef CMAKE_H
#define CMAKE_H

/* Is SDL available? (should be in all cases) */
#cmakedefine HAVE_SDL

/* Is SDL_image available? (same as SDL, should be available) */
#cmakedefine HAVE_SDL_IMAGE

/* Is SDL_ttf available? (same as SDL, should be available) */
#cmakedefine HAVE_SDL_TTF

/* Is SDL_mixer available? */
#cmakedefine HAVE_SDL_MIXER

#define INSTALL_SUBDIR_SHARE "@INSTALL_SUBDIR_SHARE@"

/* Name of the generated executable. */
#define EXECUTABLE "@EXECUTABLE@"

#endif
